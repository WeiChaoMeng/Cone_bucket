<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiaoke.web.dao.PermissionMapper">

    <select id="fuzzyQueryByDescription" parameterType="java.lang.String" resultType="com.jiaoke.bean.Permission">
        select
            id, url, description
        from permission
        where description LIKE "%"#{description}"%"
    </select>
</mapper>