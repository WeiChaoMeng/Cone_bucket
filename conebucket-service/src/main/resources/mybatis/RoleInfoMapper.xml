<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiaoke.web.dao.RoleInfoMapper">

    <select id="fuzzyQueryByRoleName" parameterType="java.lang.String" resultType="com.jiaoke.bean.RoleInfo">
        select
            id, role_name as roleName, description, create_time as createTime
        from role_info
        where role_name LIKE "%"#{roleName}"%"
    </select>
</mapper>